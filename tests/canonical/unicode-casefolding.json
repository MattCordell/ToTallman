    },
    {
      "description": "Greek letters in context",
      "input": "Σ morphine",
      "listId": "DEFAULT",
      "expected": "Σ morphine"
    },
    {
      "description": "Cyrillic characters in context",
      "input": "Д morphine В",
      "listId": "DEFAULT",
      "expected": "Д morphine В"
    },
    {
      "description": "Mixed scripts",
      "input": "测试 prednisone test",
      "listId": "DEFAULT",
      "expected": "测试 predniSONE test"
    }
  ]
